[Unit]
Description=Subordination daemon
After=network.target

[Service]
User=@project_name@
Group=@project_name@
EnvironmentFile=-/etc/sysconfig/@project_name@
ExecStart=@prefix@/@sbindir@/@subordination_exe@ $SUBORDINATION_ARGS
Restart=always
RestartSec=7
AmbientCapabilities=CAP_SETUID CAP_SETGID

[Install]
WantedBy=multi-user.target
